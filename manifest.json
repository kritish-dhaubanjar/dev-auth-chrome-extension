{
  "name": "Dev Auth Extension",
  "description": "Auth Extension for dev.vyaguta.lftechnology.com",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./icons/16x16.png",
    "32": "./icons/32x32.png",
    "48": "./icons/48x48.png",
    "64": "./icons/64x64.png",
    "128": "./icons/128x128.png"
  },
  "background": {
    "scripts": ["./src/background.js"]
  },
  "options_page": "./components/options/options.html",
  "browser_action": {
    "default_popup": "./components/popup/popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://localhost/",
    "http://127.0.0.1/",
    "*://dev.vyaguta.lftechnology.com/",
    "storage"
  ]
}
